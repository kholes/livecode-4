<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<ul>
		<li><a href="/">Home</a></li>
		<li><a href="/items">Items</a></li>
		<li><a href="/suppliers">Suppliers</a></li>
	</ul>
	<p><a href="/suppliers/add">Add Suppliers</a></p>
	<table>
		<th>ID</th>
		<th>Kota</th>
		<th>Name</th>
		<th>Action</th>
		<%data.forEach(i => {%>
		<tr>
			<td><%-i.id%></td>
			<td><%-i.name%></td>
			<td><%-i.kota%></td>
			<td><a href="/suppliers/edit/<%-i.id%>">Edit</td>
			<td><a href="/suppliers/delete/<%-i.id%>">Delete</td>
		</tr>
		<%})%>
	</table>
</body>
</html>